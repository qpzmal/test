<div class="money-con mt-list">
        <h3>消费金额阶梯统计</h3>
        <input type="button" value="导出" class="money-btn" onclick="exportWayData();">
    </div>
    <!--列表-->
<table class="money-data mb-30 mt-10">
    <thead>
        <tr>
            <th rowspan="2">日期</th>
            <th colspan="2">200以内</th>
            <th colspan="2">200-400</th>
            <th colspan="2">400-600</th>
            <th colspan="2">600以上</th>
        </tr>
        <tr>
            <th>次数</th>
            <th>用户数</th>
            <th>次数</th>
            <th>用户数</th>
            <th>次数</th>
            <th>用户数</th>
            <th>次数</th>
            <th>用户数</th>
        </tr>
     </thead>
     <tbody id="tableBody">
     </tbody>        
</table>
<div class="tcdPageCodes">
</div>
<link rel="stylesheet" href="${rc.contextPath}/static/css/page.css">
<script src="${rc.contextPath}/static/js/jquery.page.js"></script>
<script>
	function drawWayPage(){

        setWayPageData(1);//加载后即展示第一页数据
        createWayPageWidget();
	}
	
	//创建分页插件, 只应该调用一次
	function createWayPageWidget(){
		var totalPage = 0;
		//获取总页数
		$.ajax({
			url: "${rc.contextPath}/zwsc/consume/getwaytotalpages.json",
            type: "POST",
            data: {
                startTime: $("#startTime").val(),
                endTime: $("#endTime").val(),
                version: $("#versionSel").val(),
                cnids: $("#cnids").val()
            },
            success: function(returnData){
            	if(returnData.code == 1){
            		totalPage = returnData.data;
                    $(".tcdPageCodes").createPage({
                        pageCount: totalPage,
                        current:1,
                        backFn: function(p){//p为当前页号
                            setWayPageData(p);
                        }
                    });
            	} else{
            		alert(returnData.info);
            		return;
            	}
            }
		});

	}
	
	function setWayPageData(pageNo){

		$.ajax({
            url: "${rc.contextPath}/zwsc/consume/getwaypagedata.json",
            type: "POST",
            data: {
                startTime: $("#startTime").val(),
                endTime: $("#endTime").val(),
                version: $("#versionSel").val(),
                cnids: $("#cnids").val(),
                pageNo: pageNo
            },
            success: function(returnData){
                if(returnData.code == 1){
                    var tableData = "";
                    $.each(returnData.data, function(i, item){
                        tableData += "<tr><td>"
                            + item.dt +"</td><td>"
                           + item.pvLt200 +"</td><td>"
                           + item.uvLt200 + "</td><td>"
                           + item.pvLt400 +"</td><td>"
                           + item.uvLt400 + "</td><td>"
                           + item.pvLt600 + "</td><td>"
                           + item.uvLt600+"</td><td>"
                           + item.pvGt600 +"</td><td>"
                            + item.uvGt600 +"</td></tr>";
                    });
                    $("#tableBody").html(tableData);
                } else{
                    alert(returnData.info);
                }
            }
        });
	}

	function exportWayData(){
        var startTime = $("#startTime").val();
        var endTime = $("#endTime").val();
        var version = $("#versionSel").val();
        var cnids = $("#cnids").val();
        window.location.href="/zwsc/consume/exportway.json?startTime="+startTime+"" +
                "&endTime="+endTime+"&version="+version+"&cnids="+cnids+"";
	}
</script>