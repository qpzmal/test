<form class="form-horizontal m-t" id="commentForm">
    <input type="hidden" id="flowType" name="flowType" value="save">

    <div class="wrapper wrapper-content animated fadeInRight">
        <input type="hidden" id="id" name="id" value="$baseExecuteOrder.id">
        <div class="row">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>项目基本信息</h5>
                </div>
                <div class="ibox-content">
                    <div class="form-group">
                        <label class="col-sm-3 control-label">项目编号</label>
                        <div class="col-sm-8">
                            <input value="$!{baseExecuteOrder.orderNum}" id="order_num" name="order_num" maxlength="15" type="text" class="form-control" disabled >
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">项目补充编号</label>
                        <div class="col-sm-8">
                            <input value="$!{baseExecuteOrder.secOrderNum}" id="sec_order_num" name="sec_order_num" maxlength="15" type="text" class="form-control"
                                #if($!{hasSaleAuth} == false)
                                   disabled
                                #end
                            >
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">项目名称</label>
                        <div class="col-sm-8">
                            <input value="$!{baseExecuteOrder.name}" id="p_name" name="p_name" maxlength="50" type="text" class="form-control" required aria-required="true"
                                #if($!{hasSaleAuth} == false)
                                   disabled
                                #end
                            >
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">项目日期</label>
                        <div class="col-sm-8">
                            <input value="$!{format.format($!{baseExecuteOrder.orderDate})}" id="order_date" name="order_date" class="laydate-icon form-control layer-date" required aria-required="true"
                                #if($!{hasSaleAuth} == false)
                                   disabled
                                #end
                            >
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">区域</label>
                        <div id="area_content" class="col-sm-9">
                            <input value="$!{baseExecuteOrder.areaId}" id="area_id" hidden >
                            <input value="$!{areaName}" id="area_name" name="area_name" type="text" class="form-control"
                                #if(!${areaTreeJson})
                                   disabled
                                #end
                                   onclick="showAreaList(); return false;" onfocus=this.blur() required aria-required="true"
                                #if($!{hasSaleAuth} == false)
                                   disabled
                                #end
                            >
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">项目负责人</label>
                        <div id="person_leader_content" class="col-sm-8">
                            <select id="person_leader_id" ata-placeholder="请选择项目负责人" class="chosen-select" style="width:350px;" tabindex="2"
                                #if($!{hasSaleAuth} == false)
                                    disabled
                                #end
                            >
                                <option value="">请选择项目负责人</option>
                                #foreach($data in $leaderList)
                                    <option value ="$data.id"
                                            #if(${baseExecuteOrder.personLeaderId} == $data.id)
                                            selected
                                            #end
                                            >$data.name</option>
                                #end
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>客户信息</h5>
                </div>
                <div class="ibox-content">
                    <div class="form-group">
                        <label class="col-sm-3 control-label">签约类型</label>
                        <div class="col-sm-8">
                            <select id="sign_type" data-placeholder="选择签约类型..." class="chosen-select" style="width:350px;"  tabindex="2" onchange="signTypeChange()"
                                #if($!{hasSaleAuth} == false)
                                    disabled
                                #end
                            >
                                <option value="">请选择签约类型</option>
                                <option value="1" hassubinfo="true"
                                    #if(${baseExecuteOrder.signType} == 1)
                                        selected
                                    #end
                                >代理</option>
                                <option value="2" hassubinfo="true"
                                    #if(${baseExecuteOrder.signType} == 2)
                                        selected
                                    #end
                                >直客</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">签约公司</label>
                        <div id="sign_company_content" class="col-sm-8">
                            <select id="sign_company_select" ata-placeholder="请选择签约公司" class="form-control" style="width:350px;" tabindex="2"
                                #if($!{hasSaleAuth} == false)
                                    disabled
                                #end>
                                <option value="">请选择签约公司</option>
                                #foreach($data in $signCompanyList)
                                    <option value ="$data.id"
                                        #if(${baseExecuteOrder.customSignId} == "$data.id")
                                            selected
                                        #end
                                    >$data.name</option>
                                #end
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">客户行业</label>
                        <div class="col-sm-8">
                            <select id="custom_industry_id" ata-placeholder="请选择客户行业" class="chosen-select" style="width:350px;" tabindex="2"
                                #if($!{hasSaleAuth} == false)
                                    disabled
                                #end
                            >
                                <option value="">请选择客户行业</option>
                                #foreach($data in $industryList)
                                    <option value ="$data.id"
                                            #if("$data.id" == $!{baseExecuteOrder.industryId})
                                            selected
                                            #end
                                    >$data.name</option>
                                #end
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">客户</label>
                        <div id="custom_content" class="col-sm-8">
                            <select id="custom_select" ata-placeholder="请选择客户" class="form-control" style="width:350px;" tabindex="2"
                                #if($!{hasSaleAuth} == false)
                                    disabled
                                #end>
                                <option value="">请选择客户</option>
                                #foreach($data in $customList)
                                    <option value ="$data.id"
                                        #if("$data.id" == $!{baseExecuteOrder.customAdverserId})
                                            selected
                                        #end
                                    >$data.name</option>
                                #end
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>投放信息</h5>
                </div>
                <div class="ibox-content">
                    <div class="form-group">
                        <label class="col-sm-3 control-label">投放地区编号</label>
                        <div class="col-sm-8">
                            <select id="delivery_area_ids" data-placeholder="选择投放地区" class="chosen-select" multiple style="width:350px;" tabindex="4"
                                #if($!{hasMediaAuth} == false)
                                    disabled
                                #end
                            >
                                <option value="">请选择投放地区</option>
                                #foreach($data in $regionList)
                                    <option value ="$data.id"
                                    #if($selectedReginList.contains("$data.id"))
                                        selected
                                    #end
                                    >$data.name</option>
                                #end
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">投放开始日期</label>
                        <div class="col-sm-4">
                            <input value="$!{format.format($!{baseExecuteOrder.startDate})}" id="start_date" placeholder="开始日期" class="laydate-icon form-control layer-date"
                                #if($!{hasMediaAuth} == false)
                                   disabled
                                #end
                            >
                        </div>
                        <div class="col-sm-4">
                            <input value="$!{format.format($!{baseExecuteOrder.endDate})}" id="end_date" placeholder="结束日期" class="laydate-icon form-control layer-date"
                                #if($!{hasMediaAuth} == false)
                                   disabled
                                #end
                            >
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>监测信息</h5>
                </div>
                <div class="ibox-content">
                    <div class="form-group">
                        <label class="col-sm-3 control-label">监测公司（客户指定）</label>
                        <div class="col-sm-8">
                            <select id="monitor_ids" data-placeholder="请选择监测公司" class="chosen-select" multiple style="width:350px;" tabindex="2"
                                #if($!{hasSaleAuth} == false)
                                    disabled
                                #end
                            >
                                <option value="">请选择监测公司</option>
                                #foreach($data in $monitorRequestList)
                                    <option value ="$data.id"
                                        #if($selectMonitorList.contains("$data.id"))
                                            selected
                                        #end
                                    >$data.name</option>
                                #end
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">监测公司（我方指定）</label>
                        <div class="col-sm-8">
                            <select id="our_monitor_ids" data-placeholder="请选择监测公司" class="chosen-select" multiple style="width:350px;" tabindex="2"
                                #if($!{hasMediaAuth} == false)
                                    disabled
                                #end
                            >
                                <option value="">请选择监测公司</option>
                                #foreach($data in $monitorRequestList)
                                    <option value ="$data.id"
                                        #if($selectOurMonitorNameList.contains($data.name))
                                            selected
                                        #end
                                    >$data.name</option>
                                #end
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">报告类型</label>
                        <div class="col-sm-8">
                            <select id="report_type_id" data-placeholder="选择报告类型..." class="chosen-select" multiple style="width:350px;"  tabindex="2"
                                #if($!{hasMediaAuth} == false)
                                    disabled
                                #end
                            >
                                <option value="">请选择报告类型</option>
                                <option value="1" hassubinfo="true"
                                    #if($selectReportList.contains("1"))
                                        selected
                                    #end
                                >日报</option>
                                <option value="2" hassubinfo="true"
                                    #if($selectReportList.contains("2"))
                                        selected
                                    #end>周报</option>
                                <option value="3" hassubinfo="true"
                                    #if($selectReportList.contains("3"))
                                        selected
                                    #end>月报</option>
                                <option value="4" hassubinfo="true"
                                    #if($selectReportList.contains("4"))
                                        selected
                                    #end>年报</option>
                                <option value="5" hassubinfo="true"
                                    #if($selectReportList.contains("5"))
                                        selected
                                    #end>结案报告</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>销售信息</h5>
                </div>
                <div class="ibox-content">
                    <div class="form-group">
                        <label class="col-sm-3 control-label">销售负责人</label>
                        <div class="col-sm-8">
                            <input value="$!{salePersonId}" id="person_sales_id" hidden>
                            <input value="$!{salePersonName}" id="person_sales_name" name="person_sales_name" maxlength="100" type="text" class="form-control" aria-required="true" disabled>
                        </div>
                    </div>
##                    <div class="form-group">
##                        <label class="col-sm-3 control-label">项目总金额</label>
##                        <div class="col-sm-8">
##                            <input value="$!{baseExecuteOrder.amount}" id="amount" name="amount" type="text" class="form-control isMoney" aria-required="true"
##                                #if($!{hasSaleAuth} == false)
##                                   disabled
##                                #end
##                            >
##                        </div>
##                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">对公返点</label>
                        <div class="col-sm-8">
                            <input value="$baseExecuteOrder.getPublicRebate().multiply($std)" id="public_rebate" name="public_rebate" type="text" class="form-control isPercent"
                                #if($!{hasSaleAuth} == false)
                                   disabled
                                #end
                            >
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">对私返点</label>
                        <div class="col-sm-8">
                            <input value="$baseExecuteOrder.getPrivateRebate().multiply($std)" id="private_rebate" name="private_rebate" type="text" class="form-control isPercent"
                                #if($!{hasSaleAuth} == false)
                                   disabled
                                #end
                            >
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">含税金额</label>
                        <div class="col-sm-8">
                            <input value="$!{baseExecuteOrder.taxAmount}" id="tax_amount" name="tax_amount" type="text" class="form-control isMoney"
                                #if($!{hasSaleAuth} == false)
                                   disabled
                                #end
                            >
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">不含税金额</label>
                        <div class="col-sm-8">
                            <input value="$!{baseExecuteOrder.taxFreeAmount}" id="tax_free_amount" name="tax_free_amount" type="text" class="form-control isMoney"
                                #if($!{hasSaleAuth} == false)
                                   disabled
                                #end
                            >
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>付款信息 </h5>
                </div>
                <div class="ibox-content">
                    <div class="form-group">
                        <label class="col-sm-3 control-label">收款负责人</label>
                        <div class="col-sm-8">
                            <input value="$!{baseExecuteOrder.personPayeeId}" id="person_payee_id" maxlength="30" name="person_payee_id" type="text" class="form-control"
                                #if($!{hasSaleAuth} == false)
                                   disabled
                                #end
                            >
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">付款比例</label>
                        <div class="col-sm-8 ">
                            <input value="$baseExecuteOrder.getPayPercent().multiply($std)" id="pay_percent" name="pay_percent" type="text" class="form-control isPercent"
                                #if($!{hasFinancialAuth} == false)
                                   disabled
                                #end
                            >
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">账期</label>
                        <div class="col-sm-8">
                            <input value="$!{baseExecuteOrder.accountPeriod}" id="account_period" name="account_period" maxlength="5" type="text" class="form-control"
                                #if($!{hasSaleAuth} == false)
                                   disabled
                                #end
                            >
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>CPM信息</h5>
                </div>
                <div class="ibox-content">
                    <div class="btn-group hidden-xs" id="exampleTableEventsToolbar" role="group">
                        <button id="btn_plus" type="button" class="btn btn-outline btn-default">
                            <i class="glyphicon glyphicon-plus" aria-hidden="true"></i>
                        </button>
                        <button id="btn_trash" type="button" class="btn btn-outline btn-default">
                            <i class="glyphicon glyphicon-trash" aria-hidden="true"></i>
                        </button>
                    </div>
                    <table id="exampleTableEvents" data-height="400" data-mobile-responsive="true">
                        <thead>
                        <tr>
                            <th data-field="state" data-checkbox="true"></th>
                            <th data-field="num" data-editable="true">行号</th>
                            <th data-field="mediaId" data-editable="true">媒体Id</th>
                            <th data-field="mediaPrice" data-editable="true" >媒体单价</th>
                            <th data-field="firstPrice" data-editable="true" >媒体采购成本单价</th>
                            <th data-field="adTypeId" data-editable="true" >广告形式Id</th>
                            <th data-field="cpm" data-editable="true" >CPM数</th>
                            <th data-field="remark" data-editable="true" >备注</th>
                            <th data-field="id" data-editable="true">id</th>
                        </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="form-group">
                <div class="col-sm-2 col-sm-offset-2">
                    <button class="btn btn-primary" type="submit"><i class="fa fa-check"></i> 保存</button>
                </div>
                <div class="col-sm-2 col-sm-offset-2">
                    <button class="btn btn-info" type="button"><i class="fa fa-play"></i> 发起流程</button>
                </div>
                <div class="col-sm-2 col-sm-offset-2">
                    <button id="btn_cancel" class="btn btn-default" type="button"><i class="fa fa-reply"></i> 取消</button>
                </div>
            </div>
        </div>


    </div>
</form>

