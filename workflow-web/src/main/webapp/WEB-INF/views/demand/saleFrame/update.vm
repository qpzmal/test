<script>

    function areaChange() {
        $.ajax({
            url:"/saleOrder/leaderSelect.json?areaId=" + $("#area_id").val(),
            type:"get",
            success: function(returnData){
                console.log(returnData);
                $("#person_leader_content").html(returnData);
            }
        });
    }

    ##签约类型变更
    function signTypeChange(){
        $.ajax({
            url:"/saleOrder/signCompanySelect.json?signType=" + $("#sign_type").val(),
            type:"get",
            success: function(returnData){
                console.log(returnData);
                $("#sign_company_content").html(returnData);
            }
        });
    }

    $('#sign_company_select').editableSelect({
        effects: 'slide',
        onSelect:function (element) {
            signCompanyChange();
        }
    });

    $('#custom_select').editableSelect({
        effects: 'slide'

    });

    ##签约公司变更
    function signCompanyChange(){
        $.ajax({
            url:"/saleOrder/customSelect.json?signCustomName=" + $("#sign_company_select").val(),
            type:"get",
            success: function(returnData){
                console.log(returnData);
                $("#custom_content").html(returnData);
            }
        });
    }

    ##显示公司列表
    function onBodyDown(event) {
        if (!(event.target.id == "area_id" || event.target.id == "area_tree_content" || $(event.target).parents("#area_tree_content").length>0)) {
            hideMenu();
        }
    }
    function hideMenu() {
        $("#area_tree_content").fadeOut("fast");
        $("body").unbind("mousedown", onBodyDown);
    }
    function showAreaList() {
        var area_name = $("#area_name");
        var area_content_offSet = $("#area_content").offset();
        var area_name_offSet = area_name.offset();
        $("#area_tree_content").css({left:area_name_offSet.left + "px", top:area_content_offSet.top + area_name.outerHeight() + "px"}).slideDown("fast");
        $("body").bind("mousedown", onBodyDown);
    };
    ##显示公司列表

    var cpmTable =$("#exampleTableEvents");
    $("#btn_plus").click(
        function () {
            var index = cpmTable.bootstrapTable('getData').length;
            var currentId ;
            if (index > 1) {
                currentId = cpmTable.bootstrapTable('getData')[index-1].num;
            } else if (index == 1) {
                currentId = cpmTable.bootstrapTable('getData')[0].num;
            } else {
