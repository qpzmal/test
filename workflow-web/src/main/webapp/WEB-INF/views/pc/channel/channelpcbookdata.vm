#set($layout = '/layout/empty.vm')
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!--360浏览器优先以webkit内核解析-->
    <title> - 主页示例</title>
    <link rel="shortcut icon" href="favicon.ico">
    <link href="${rc.contextPath}/static/css/bootstrap.min.css?v=3.3.6" rel="stylesheet">
    <link href="${rc.contextPath}/static/css/font-awesome.css?v=4.4.0" rel="stylesheet">
    <link href="${rc.contextPath}/static/css/animate.css" rel="stylesheet">
    <link href="${rc.contextPath}/static/css/style.css?v=4.1.0" rel="stylesheet">
    <link href="${rc.contextPath}/static/js/plugins/layer/laydate/need/laydate.css" rel="stylesheet">
</head>
<body class="white-bg">
<div class="wrapper wrapper-content">
<div class="top">
    <form id="form1" action="/pc/channel/pcbookdata"  method="post" name="pageForm">
        <input type="hidden"  name="pageSize" id="pageSize" value="$!{page.pageSize}" ></input>
        <input type="hidden"  name="pageIndex" id="pageIndex" value="$!{page.pageIndex}" ></input>
    <div class="top-r">
        <label>*选择时间：</label>
        <input type="text" id="startTime" name="startTime" class="datepicker" value="$!{startTime}">&nbsp;&nbsp;——&nbsp;&nbsp;
        <input type="text" id="endTime" name="endTime" class="datepicker" value="$!{endTime}">
        <label class="ml20">作品ID：</label>
        <input type="text" class="inp-name" name="bookId" id="cnid" value="$!{bookId}">
        <label class="ml20">作品名：</label>
        <input type="text" class="inp-name" name="bookName" id="bookName" value="$!{bookName}">
        <input type="submit" value="  查  询  " class="inp-sure" />
    </div>
    </form>
</div>
#*<div class="money-con mt-list">
        <h3>数据总览：</h3>
        <input type="button" value="导出" class="money-btn" onclick="exportDetailData();">
    </div>*#
    <!--列表-->
<table class="table table-bordered">
    <tbody>
        <tr>
            <th >作品ID</th>
            <th >作品名</th>
            <th>点击uv</th>
            <th>详情页uv</th>
            <th>目录页uv</th>
            <th>阅读页uv</th>
            <th>书架收藏uv</th>
            <th>订阅uv</th>
            <th>订阅钱数(元)</th>
            <th>订阅真钱(元)</th>
            <th>打赏uv</th>
            <th>打赏钱数(元)</th>
            <th>真钱打赏(元)</th>
        </tr>
     </tbody>
      #foreach($PcBookReport in $!{page.getData()})
        <tr>
          #*  private Integer id;
            private String bookId;
            private String bookName;
            private Integer consumerUser;
            private Integer consumerAllMoney;
            private Integer consumerMoney;
            private Integer dsUser;
            private Integer dsAllMoney;
            private Integer dsMoney;
            private Integer bookUser;
            private Integer catalogPageUser;//目录页uv
            private Integer bookPageUser;//图书详情页uv
            private Integer enshineUser;
            private String dt;*#
            <td>$!{PcBookReport.bookId}</td>
            <td>$!{PcBookReport.bookName}</td>
            <td>$!{PcBookReport.bookUser}</td>
            <td>$!{PcBookReport.bookPageUser}</td>
            <td>$!{PcBookReport.catalogPageUser}</td>
            <td>$!{PcBookReport.chapterPageUser}</td>
            <td>$!{PcBookReport.enshineUser}</td>
            <td>$!{PcBookReport.consumerUser}</td>
            <td>$!{PcBookReport.consumerAllMoney}</td>
            <td>$!{PcBookReport.consumerMoney}</td>
            <td>$!{PcBookReport.dsUser}</td>
            <td>$!{PcBookReport.dsAllMoney}</td>
            <td>$!{PcBookReport.dsMoney}</td>
        </tr>
      #end
</table>
<div class="tcdPageCode">
    #parse('layout/page.vm')
</div>
</div>
</body>
<script src="${rc.contextPath}/static/js/jquery.min.js?v=2.1.4"></script>
<script src="${rc.contextPath}/static/js/bootstrap.min.js?v=3.3.6"></script>
<script src="${rc.contextPath}/static/js/plugins/layer/laydate/laydate.js"></script>
<!-- 自定义js -->
<script src="${rc.contextPath}/static/js/pc/channel.js"></script>
<script>
    function exportDetailData(){
        var startTime = "$!{dt}";
        var endTime = "$!{dt}";
        var version = $("#versionSel").val();
        var cnids = $("#cnids").val();
        window.location.href="/zwsc/channel/exportDetailData.json?startTime="+startTime+"" +
                "&endTime="+endTime+"&version="+version+"&cnids="+cnids+"";

    }
</script>
