        /**
         * 返回选中的功能ID的JSONStr
         *
         */
        function getSelectedFunctionJsonStr() {
            var functionArr = $(".function");
            var selectedFunctionIds = new Array();
            var selectedFunctionArrIndex = 0;
            for (var i = 0; i < functionArr.length; i++) {
                if (functionArr[i].checked) {
                    selectedFunctionIds[selectedFunctionArrIndex] = (functionArr[i].id);
                    selectedFunctionArrIndex = selectedFunctionArrIndex +1;
                }
            };
            var selectedFunctionJsonStr = JSON.stringify(selectedFunctionIds);
            return selectedFunctionJsonStr;
        }

